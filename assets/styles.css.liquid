========================================================================== */
html {
  font-size: 1rem;
  scroll-behavior: smooth;
}

body {
  font-family: "Lyon Text", helvetica, sans-serif;
  font-weight: normal;
  font-stretch: normal;
  line-height: 1.87;
  color: #555555;
}

iframe[src="//localhost:3000/preview_bar"] {
  display: none;
}

.visually-hidden {
  position: absolute !important;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px;
  width: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
}

