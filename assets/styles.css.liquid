  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.1;
  color: #333;
  font-size: 4.375rem;
}

label {
  color: #969696;
  font-family: "Styrene M", helvetica, sans-serif;
  font-size: 0.625rem;
  font-weight: bold;
  letter-spacing: 0.03125rem;
  line-height: 1.2;
  text-transform: uppercase;
}

